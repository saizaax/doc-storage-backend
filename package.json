{
  "name": "doc-storage-backend",
  "version": "1.0.0",
  "description": "Document management backend service",
  "main": "src/index.ts",
  "scripts": {
    "build": "npx tsc && tsc-alias",
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "format": "prettier --write ./src",
    "start:prod": "node dist/index.js",
    "start:prod:migrate": "prisma migrate deploy && npm run start:prod",
    "generate": "prisma generate",
    "migrate": "prisma migrate dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saizaax/doc-storage-backend.git"
  },
  "keywords": [
    "documents",
    "nodejs",
    "prisma",
    "typescript"
  ],
  "author": "saizaax",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/saizaax/doc-storage-backend/issues"
  },
  "homepage": "https://github.com/saizaax/doc-storage-backend#readme",
  "dependencies": {
    "@prisma/client": "^4.8.1",
    "axios": "^1.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/node": "^18.11.18",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.1",
    "prisma": "^4.8.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.2",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.4"
  }
}
